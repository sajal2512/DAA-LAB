#include<stdio.h>
void read1(int arr[],int n){
	printf("Enter %d elements : ",n);
	for(int i=0;i<n;i++){
		scanf("%d",&arr[i]);
	}
}
int linearSearch(int arr[],int pos,int n, int target){
	if(pos>n)
		return -1;
	if(arr[pos] == target){
		return pos;
	}
	return linearSearch(arr, pos+1, n,target);
}

